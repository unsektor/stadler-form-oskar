{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ES2020", "DOM"],
    "allowJs": true,
    "outDir": "./public/assets",
    "rootDir": "./frontend_src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": ["frontend_src/**/*"],
  "exclude": ["node_modules", "dist"]
}
